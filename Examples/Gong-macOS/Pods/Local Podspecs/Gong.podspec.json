{
  "name": "Gong",
  "version": "0.2.0",
  "summary": "Gong is a MIDI library for iOS and macOS.",
  "homepage": "https://github.com/dclelland/Gong",
  "license": {
    "type": "MIT"
  },
  "authors": {
    "Daniel Clelland": "daniel.clelland@gmail.com"
  },
  "source": {
    "git": "https://github.com/dclelland/Gong.git",
    "tag": "0.2.0"
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.10"
  },
  "subspecs": [
    {
      "name": "Audio",
      "source_files": "Sources/Audio/*.swift",
      "subspecs": [
        {
          "name": "Core",
          "source_files": "Sources/Audio/**/*.swift",
          "frameworks": "AudioToolbox"
        }
      ]
    },
    {
      "name": "MIDI",
      "source_files": "Sources/MIDI/*.swift",
      "subspecs": [
        {
          "name": "Core",
          "source_files": "Sources/MIDI/Core/**/*.swift",
          "frameworks": "CoreMIDI"
        },
        {
          "name": "Events",
          "source_files": "Sources/MIDI/Events/**/*.swift",
          "dependencies": {
            "Gong/MIDI/Core": [

            ]
          }
        }
      ]
    }
  ]
}
